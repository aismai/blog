<div class="skinny-wrapper">
    <h3>Create User:</h3>
    <div>
        <label>Email</label>
        {{input type="text" value=model.email class="form-control" placeholder="email:"}}
    </div>
    <div>
        <label>Password</label>
        {{input type="password" value=model.password class="form-control" placeholder="password:"}}
    </div>
    <div>
        <input type="submit" {{action 'save' model}} placeholder="create">
    </div>
</div>
