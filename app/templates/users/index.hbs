<h3>users</h3>
<table>
    <tr>
        <th>email</th>
        <th>blogs</th>
        <th>posts</th>
        <th>comments</th>
        <th>registration</th>
        <th>login</th>
    </tr>

        {{#each model as |user|}}
        <tr>
            <td>{{user.email}}</td>
            <td>{{link-to user.blogs.length 'blogs'  (query-params user= user.id)}}</td>
            <td>{{user.posts.length}}</td>
            <td>{{user.comments.length}}</td>
            <td>{{user.registration}}</td>
            <td>{{user.lastLogin}}</td>
        </tr>
        {{/each}}

</table>